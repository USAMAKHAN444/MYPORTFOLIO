{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/portfolio-usamashakeel.html"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "/portfolio-usamashakeel.html",
      "permanent": false
    }
  ]
} 