{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/portfolio-usamashakeel.html"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/portfolio-usamashakeel.html"
    },
    {
      "src": "/index.html",
      "dest": "/portfolio-usamashakeel.html"
    }
  ]
} 